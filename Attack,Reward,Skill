getgenv().Attack = true
getgenv().Validate = true
getgenv().Reward = true
spawn(function()
while Attack == true do
game:GetService("ReplicatedStorage").Remotes.Attack:FireServer() 
wait(0.1)
end
end)

spawn(function()
while Validate == true do 
local args = {
    [1] = "One"
}

game:GetService("ReplicatedStorage").Remotes.Validate:FireServer(unpack(args))
wait(0.1)
end
end)

spawn(function()
while Validate == true do 
local args = {
    [1] = "Two"
}

game:GetService("ReplicatedStorage").Remotes.Validate:FireServer(unpack(args))
wait(0.1)
end
end)

spawn(function()
while Validate == true do 
local args = {
    [1] = "Three"
}

game:GetService("ReplicatedStorage").Remotes.Validate:FireServer(unpack(args))
wait(0.1)
end
end)

