getgenv().T = true
spawn(function()
while T == true do
local args = {
    [1] = "Change_Value",
    [2] = workspace.Debree.clash_folder.FamiliarYeti391vsEnmu.FamiliarYeti391,
    [3] = 100
}

game:GetService("ReplicatedStorage").Remotes.To_Server.Handle_Initiate_S:FireServer(unpack(args))
wait()
end
end)
