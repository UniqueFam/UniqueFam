getgenv().autoUnstore = false;
local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()
local w = library:CreateWindow("Blox Fruit")
local b = w:CreateFolder("AutoUnstore")
b:Toggle("AutoUnstore",function(bool)
     getgenv(),autoUnstore = bool
if bool then
    Unstore();
   end
end)

function Unstore()
spawn(function()
    while autoUnstore == true do
local args = {
    [1] = "LoadFruit",
    [2] = "Spin-Spin"
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
    wait(60.0)
    end
end)

spawn(function()
while autoUnstore == true do
local args = {
    [1] = "LoadFruit",
    [2] = "Chop-Chop"
}

game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
      wait(60.0)
    end
end)

spawn(function()
while autoUnstore == true do
local args = {
    [1] = "LoadFruit",
    [2] = "Spike-Spike"
}

