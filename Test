getgenv().Settings = {
['Name'] = 'Original Hub',
['Intro'] = true,
['Keybind'] = 'G'
}

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/kickTh/New-Ui/main/Shadow%20Lib%20v2.txt", true))()


local Tab1 = Library:CreateTab('Main')

    Tab1:Button("Back/Front Flip", "Makes you do gymnastics", function()
        loadstring(game:HttpGet('https://pastebin.com/raw/7wDcPtLk'))()
    end)
 
   Tab1:Toggle("AutoUnstore", "go fast and jump high", function(state)
if state then
loadstring(game:HttpGet("https://pastebin.com/raw/gAWBpDRS"))()
        else
loadstring(game:HttpGet("https://pastebin.com/raw/UAJq1qys"))()
        end
    end)
